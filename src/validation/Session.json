{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Session",
  "definitions": {
    "Session": {
      "type": "object",
      "properties": {
        "taxationType": {
          "$ref": "#/definitions/TaxationType"
        },
        "positionTax": {
          "$ref": "#/definitions/PositionTax"
        },
        "operator": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "vatin": {
              "type": "string"
            }
          },
          "required": ["name"],
          "additionalProperties": false
        }
      },
      "required": ["taxationType", "operator", "positionTax"],
      "additionalProperties": false
    },
    "TaxationType": {
      "type": "string",
      "enum": ["osn", "usnIncome", "usnIncomeOutcome", "envd", "esn", "patent"]
    }
  }
}
